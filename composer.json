{
    "name": "navarr/fig-hug-interfaces",
    "description": "Interfaces for PSR-8",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "provide": {
        "psr/hug": "1.0-dev"
    },
    "authors": [
        {
            "name": "Navarr Barnier",
            "email": "me@navarr.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Psr\\Hug\\": "src/"
        }
    },
    "archive": {
        "exclude": [
            ".composer/",
            ".github/",
            ".env",
            ".gitignore",
            "composer.lock",
            "phpcs.xml.dist"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6"
    }
}
